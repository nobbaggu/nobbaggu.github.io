        <span class="n">isRun</span> <span class="o">=</span> <span class="kc">false</span><span class="o">;</span> <span class="c1">// ì„œë¸Œ ìŠ¤ë ˆë“œì˜ whileë¬¸ íƒˆì¶œ</span>
